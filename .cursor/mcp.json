{
  "servers": {
    "vercel": {
      "command": "npm",
      "args": ["run", "mcp:vercel"],
      "env": {
        "VERCEL_TOKEN": "${VERCEL_TOKEN}",
        "VERCEL_TEAM_ID": "${VERCEL_TEAM_ID}",
        "VERCEL_PROJECT_ID": "${VERCEL_PROJECT_ID}"
      }
    },
    "github": {
      "command": "npm",
      "args": ["run", "mcp:github"],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}",
        "GITHUB_USERNAME": "${GITHUB_USERNAME}",
        "GITHUB_ORG": "${GITHUB_ORG}"
      }
    }
  }
}


